{
    "name": "osmianski/laravel-extensibility",
    "description": "Allows 3rd parties extend your classes",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com",
            "homepage": "https://osmianski.com/"
        }
    ],
    "homepage": "https://github.com/osmianski/laravel-extensibility",
    "keywords": ["Laravel", "Extensibility"],
    "require": {
        "illuminate/support": "~10"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Osmianski\\Extensibility\\": "src/",
            "Osmianski\\Extensibility\\Samples\\": "samples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osmianski\\Extensibility\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osmianski\\Extensibility\\ExtensibilityServiceProvider"
            ]
        }
    }
}
