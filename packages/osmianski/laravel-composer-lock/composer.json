{
    "name": "osmianski/laravel-composer-lock",
    "description": "Utilities for `composer.lock` processing",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com",
            "homepage": "https://osmianski.com/"
        }
    ],
    "homepage": "https://github.com/osmianski/laravel-composer-lock",
    "keywords": ["Laravel", "Composer"],
    "require": {
        "illuminate/support": "~10"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Osmianski\\ComposerLock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osmianski\\ComposerLock\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osmianski\\ComposerLock\\ComposerLockServiceProvider"
            ]
        }
    }
}
